
{
  "name": "VK - рекомендации аудио из LastFM",
  "description" : "Показывает рекомендации сервиса Last FM для аудиозаписей VK",
  "version": "1.0",
  "manifest_version": 2,
  "browser_action": {
    "default_title": "LastFM similar artists in VK",
    "default_icon": "icon.png"
  },
  "permissions": [
    "storage",
    "https://vk.com/*"
  ],
  "content_scripts": [
    {
      "matches": ["https://vk.com/*"],
      "css": ["app.css"],
      "js": ["app.js"]
    }
  ]
}
